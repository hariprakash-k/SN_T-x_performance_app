function handler({
    api,
    event,
    helpers,
    imports
}) {

    // saving the form values. When save draft is pressed, the status of the record is submitted
    api.data.gform.setValue({
        fieldName: 'state',
        value: 'submitted'
    });

    api.data.gform.save();

}]]></script>
